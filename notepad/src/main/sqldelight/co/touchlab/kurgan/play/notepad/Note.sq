CREATE TABLE note(
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    title TEXT NOT NULL,
    note TEXT,
    created INTEGER NOT NULL,
    modified INTEGER NOT NULL,
    hiblob BLOB
);


insertNote:
INSERT OR FAIL INTO note(title, note, created, modified, hiblob)
VALUES (?, ?, ?, ?, ?)
;


count:
SELECT COUNT(id)
FROM note
;

selectAll:
SELECT id, title, note
FROM note
LIMIT ?
;


